{
  "name": "socialflux",
  "version": "1.0.0",
  "description": "Plataforma de marketing com IA para criação automática de campanhas visuais",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Eduardo Henrique",
    "email": "duhenri9@gmail.com",
    "url": "https://github.com/duhenri9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duhenri9/socialflux.git"
  },
  "homepage": "https://github.com/duhenri9/socialflux#readme",
  "bugs": {
    "url": "https://github.com/duhenri9/socialflux/issues"
  },
  "keywords": [
    "marketing",
    "ai",
    "social-media",
    "campaigns",
    "automation",
    "react",
    "typescript",
    "express",
    "postgresql"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "npm run check && vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist node_modules/.cache",
    "postinstall": "npm run check"
  },
